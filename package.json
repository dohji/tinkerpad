{
  "name": "tinkerpad",
  "productName": "TinkerPad",
  "version": "1.1.0",
  "description": "A modern JavaScript playground built with Electron, featuring a Monaco Editor, live console output, and external library support. TinkerPad provides a safe, sandboxed environment for experimenting with JavaScript code",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "make": "electron-forge make",
    "make:mac": "electron-forge make --platform=darwin",
    "make:win": "electron-forge make --platform=win32 --arch=x64",
    "make:win-arm": "electron-forge make --platform=win32 --arch=arm64",
    "make:linux": "electron-forge make --platform=linux --arch=x64",
    "make:linux-arm": "electron-forge make --platform=linux --arch=arm64",
    "make:all": "electron-forge make --platform=darwin,win32,linux --arch=x64,arm64"
  },
  "keywords": [],
  "author": {
    "name": "Rich Dodzi Atitsusi",
    "email": "rich.dodzi11@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-webpack": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "autoprefixer": "^10.4.21",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "electron": "38.1.2",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.13"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "fs-extra": "^11.3.2",
    "monaco-editor": "^0.53.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "uuid": "^13.0.0"
  },
  "config": {
    "forge": "./forge.config.js"
  }
}
